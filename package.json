{
  "name": "react-social-media-app",
  "version": "1.0.0",
  "description": "Social Media App with React Router, Redux Toolkit, and Cypress Tests (Node 16 Safe Versions)",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --watch --open --hot",
    "build": "webpack --mode production",
    "test": "jest --no-color 2>output.txt",
    "cypress:open": "cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "8.0.5",
    "react-router-dom": "6.8.1",
    "cypress": "9.5.0",
    "css-loader": "4.3.0",
    "style-loader": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@testing-library/react": "13.4.0",
    "babel-loader": "8.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "html-webpack-plugin": "5.5.0",
    "jest": "29.5.0",
    "jest-junit": "15.0.0",
    "react-test-renderer": "16.13.1",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "xml-js": "1.6.11"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/config/CSSStub.js"
